<h1>Request Naptime</h1>


  <%= form_for @appointment do |f| %>
  <strong><%= f.label :location %>:</strong>
  <%= f.select :location, [['Tribeca'],['Soho'],['Fidi'],['Times Square'],['Union Square']] %>    <strong><%= f.label :date %>:</strong>
    <%= f.select :date, [['January'],['February'],['March'],['April'],['May'],['June'],['July'],['August'],['September'],['October'],['November'],['December']] %>
    <strong><%= f.label :time %>:</strong>
    <%= f.select :time, [['1:00 PM'],['2:00 PM'],['3:00 PM'],['4:00 PM'],['5:00 PM'],['6:00 PM'],['7:00 PM'],['8:00 PM'],['9:00 PM'],['10:00 PM']] %>
    <br>
    <br>
    <%= f.submit %>

  <% end %>

